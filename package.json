{
  "name": "nestjs-demo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production nest start --watch",
    "start:prod-build": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "my-test": "nodemon --exec ts-node src/test.ts",
    "my-test:dev": "cross-env NODE_ENV=development node test-env.js",
    "my-test:uat": "cross-env NODE_ENV=uat node test-env.js",
    "my-test:prod": "cross-env NODE_ENV=production node test-env.js",
    "pm2-dev": "cross-env NODE_ENV=development pm2 start bin/pm2-dev.config.js",
    "pm2-prod": "cross-env NODE_ENV=production pm2 start bin/pm2-dev.config.js",
    "generate:models": "typeorm-model-generator -h 127.0.0.1 -p 3306 -d nestjs_demo -u root -x root123 -e mysql -o ./src/entities",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "migration最后再说: npx typeorm-ts-node-commonjs migration:create ./src/migrations/create-table",
    "migration:generate": "node ./scripts/migration-generate.js",
    "migration:run": "npm run build&&npx typeorm-ts-node-commonjs migration:run -d ./src/config/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/config/data-source.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.4.0",
    "mysql2": "^3.5.1",
    "nest-winston": "^1.9.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "18.16.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "config": "^3.3.9",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
